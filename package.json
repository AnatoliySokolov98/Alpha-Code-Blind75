{
  "name": "blind75",
  "version": "1.0.0",
  "description": "LeetCode Blind 75 Problems in TypeScript",
  "main": "index.js",
  "scripts": {
    "start": "tsc && node js-files/leetcode-problem.js",
    "dev": "ts-node blind75/leetcode-problem.ts",
    "build": "tsc",
    "test": "jest --watch"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/priorityqueuejs": "^1.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@datastructures-js/priority-queue": "^6.3.0",
    "priority-queue-ts": "^0.4.1",
    "priorityqueuejs": "^2.0.0",
    "queue-priority": "^1.0.0",
    "ts-data-structures": "^1.0.5",
    "ts-node": "^10.9.1",
    "typescript-collections": "^1.3.3"
  }
}
